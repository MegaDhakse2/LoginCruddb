<h1>Login</h1>


<%= form_with(scope: :session, url: login_path) do |form| %>

<div class="mb-5">
	<%= form.label :username%>
	<%= form.text_field :username, class:"form-control" %>
</div>

<div class="mb-5">
	<%= form.label :password%>
	<%= form.password_field :password, class:"form-control" %>
</div>

<div class="mb-5">
	<%= form.submit "Login", class:"btn btn-primary"%>
</div>

<%end%>
